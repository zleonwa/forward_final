/*=========================================================
  홈슬라이드박스
=========================================================*/ 
.home_slidebannerbx{clear: both;position: relative; top:0; left:0; width:100%; height: 100vh;min-height: 640px;}
.home_slidebannerbx .slide_ul > li{position: absolute; top:0; left:0; right:0; bottom:0;z-index: 1;transition: all ease-out 0.5s;-webkit-transition: all ease-out 0.5s;-moz-transition: all ease-out 0.5s;-ms-transition: all ease-out 0.5s; -o-transition: all ease-out 0.5s;}
.home_slidebannerbx .slide_itembx{position: absolute; top:0; left:0; right:0; bottom:0;}
.home_slidebannerbx .slide_bg{background: url(/wp-content/uploads/2017/04/banner.jpg) no-repeat;background-attachment: fixed; background-position: center top;background-size: cover;}

/*슬라이드 백그라운드*/
.home_slidebannerbx .bg{position: absolute; top:0; left:0; right:0; bottom:0;z-index: 10;background: rgba(0,0,0,0.2);}
.home_slidebannerbx .slide_bg{position: absolute; top:0; left:0; right:0; bottom:0;z-index: 1;}

/*텍스트 박스*/
.home_slidebannerbx .text_itembx{position: absolute; top:50%; left:0; right:0;transform: translateY(-50%);-webkit-transform: translateY(-50%);-moz-transform: translateY(-50%);-ms-transform: translateY(-50%);z-index: 20;text-align: center;}
.home_slidebannerbx .text_itembx h2{font-size: 99px; line-height: 1em; color:#fff; font-family: 'Open Sans','Nanum Gothic', sans-serif;text-transform: uppercase;font-weight: 900; text-shadow: 5px 5px 40px rgba(0,0,0,0.8);opacity: 0; visibility: hidden;padding:0 80px;max-height: 192px;}
.home_slidebannerbx .text_itembx .sub_text{font-size: 38px;  line-height: 1.2em; color:#fff; font-family: 'Open Sans','Nanum Gothic', sans-serif;text-transform: uppercase;font-weight: bold; text-shadow: 5px 5px 40px rgba(0,0,0,0.8);margin-top: 25px;padding:0 80px;max-height: 92px;}

/*슬라이드 조건값*/
.home_slidebannerbx .slide_ul > li{opacity: 0; visibility: hidden;}
.home_slidebannerbx .slide_ul > li.active{opacity: 1; visibility: visible; z-index: 10;}


/*=========================================================
  홈슬라이드박스 애니메이션
=========================================================*/ 
/*애니메이션*/
.home_slidebannerbx .delay_0{transition-delay: 100ms;-webkit-transition-delay: 100ms;-moz-transition-delay: 100ms;transition-duration: 1s;-webkit-transition-duration: 1s;-moz-transition-duration: 1s;opacity: 0;visibility: hidden;}
.home_slidebannerbx .delay_1{transition-delay: 160ms;-webkit-transition-delay: 160ms;-moz-transition-delay: 160ms;transition-duration: 1s;-webkit-transition-duration: 1s;-moz-transition-duration: 1s;opacity: 0;visibility: hidden;}
.home_slidebannerbx .delay_2{transition-delay: 220ms;-webkit-transition-delay: 220ms;-moz-transition-delay: 220ms;transition-duration: 1s;-webkit-transition-duration: 1s;-moz-transition-duration: 1s;opacity: 0;visibility: hidden;}
.home_slidebannerbx .delay_3{transition-delay: 280ms;-webkit-transition-delay: 280ms;-moz-transition-delay: 280ms;transition-duration: 1s;-webkit-transition-duration: 1s;-moz-transition-duration: 1s;opacity: 0;visibility: hidden;}
