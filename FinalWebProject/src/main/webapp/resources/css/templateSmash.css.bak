.category_two .pg_topbannerbx{background: url(../images/background/nav_two_bg.jpg) no-repeat;background-attachment: fixed;background-size: auto 324px; background-position: center top;}
.history_list{overflow: hidden;position: relative;padding:50px 0;}
.history_list:before{content: ""; width:1px; height: 120%; background: #b2b2b2; position: absolute; top:0; left:50%;margin-left:-1px;}
.history_list:after{content: ""; display: table; clear: both;}
.history_rowbx{margin-top:-24px;position: relative;}
.history_rowbx:after{content: ""; display: table; clear: both;}
.history_rowbx .history_itembx{width:50%;padding:0 35px;position: relative;z-index: 10;}
.history_rowbx .history_itembx.left{float: left;text-align:right;padding-right:50px;}
.history_rowbx .history_itembx.right{float: right;text-align: left;padding-left:50px;}
.history_rowbx .history_itembx:before{content: ""; width:14px; height: 14px;border:solid 3px #dd5828; background: #fff; border-radius: 20px;-webkit-border-radius: 20px;-moz-border-radius: 20px;display: block; position: absolute;top:4px;}
.history_rowbx .history_itembx.left:before{right:-10px;}
.history_rowbx .history_itembx.right:before{left:-10px;}
.history_rowbx .history_itembx h3{font-size: 30px; color:#dd5828; line-height: 1.2em;font-weight: bold;margin-bottom: 15px;}
.history_rowbx .history_itembx ol > li{clear: both;margin-top:10px;}
.history_rowbx .history_itembx ol > li:first-child{margin-top:0px;}
.history_rowbx .history_itembx ol > li:after{content: ""; display: table; clear: both;}
.history_rowbx .history_itembx ol > li dl:after{content: ""; display: table; clear: both;}
.history_rowbx .history_itembx.left ol > li dl{padding-right:60px;position: relative;}
.history_rowbx .history_itembx.right ol > li dl{padding-left:60px;position: relative;}
.history_rowbx .history_itembx ol > li dt{position: absolute;top:0;font-size: 16px; color:#111111; font-weight: bold; line-height: 1.4em;}
.history_rowbx .history_itembx.left ol > li dt{right:0;}
.history_rowbx .history_itembx.right ol > li dt{left:0;}
.history_rowbx .history_itembx ol > li dd li{margin-top:6px;font-size: 14px; color:#666666; line-height: 1.6em;}
.history_rowbx .history_itembx ol > li dd li:first-child{margin-top:0px;}
.history_rowbx[class*="history_"]:before{content:""; font-size: 150px; color:#666666; line-height: 1em;font-weight: bold;position: absolute; top:-20px;opacity: 0.05;}

/*조건값*/
