@charset "UTF-8";
/*=========================================================
  레이아웃
=========================================================*/ 
body.home{background: #000;}
.site_wrap{background: #fff;}
.home .site_wrap{background: #333;}
.site_wrap{overflow: hidden;}
.max_container{max-width: 1280px; padding:0 40px; margin:0 auto; position: relative;}
.max_container:after{content: ""; display: table; clear: both;}

/*=========================================================
  헤더
=========================================================*/ 
.site-header{background:rgba(0,0,0,0.7);position:fixed; top:0; left:0; width:100%; z-index: 1000;}
.admin-bar .site-header{top:32px;}
.top_headbx{position: relative;height: 62px;z-index: 10;}
.top_headbx:after{content: ""; position: absolute; bottom:0; left:0; width:100%; height: 1px; background:rgba(255,255,255,0.1);}
.nav_headbx{position: relative; clear: both;z-index: 10;}
.nav_headbx:after{content: ""; position: absolute; top:74px; left:0; width:100%; height: 1px; background:rgba(255,255,255,0.1);transition: all ease-out 0.15s; -webkit-transition: all ease-out 0.15s; -moz-transition: all ease-out 0.15s; -ms-transition: all ease-out 0.15s;-o-transition: all ease-out 0.15s;opacity: 0;visibility: hidden;}
.site-header .hover_bg{background:#000; position: absolute; top:0; left:0; right:0; height: 136px;transition: all ease-out 0.15s; -webkit-transition: all ease-out 0.15s; -moz-transition: all ease-out 0.15s; -ms-transition: all ease-out 0.15s;-o-transition: all ease-out 0.15s;opacity: 0; visibility: hidden;}

/* 로고
=========================================================*/
.logo{width:131px;position: absolute; top:14px; left:50%; margin-left:-65px;z-index: 30;}
.logo a{display: block; line-height: 0;}

/* SNS
=========================================================*/
.head_sns_bx{float: left;padding:19px 0 20px;height: 62px;}
.head_sns_bx ul:after{content: ""; display: table; clear: both;}
.head_sns_bx li{float: left;margin-left:13px;}
.head_sns_bx li:first-child{margin-left:0;}
.head_sns_bx li a{width:23px; height: 23px; text-indent: -99999px; display: block; background: url(../images/icons/icon_item.png) no-repeat;background-size: 500px 500px;}

/*SNS 조건값*/
.head_sns_bx li.instargram a{background-position:0 0;}
.head_sns_bx li.facebook a{background-position:-33px 0;}
.head_sns_bx li.youtube a{background-position:-67px 0;}

/*호버*/
.head_sns_bx li.instargram a:hover{background-position:-97px 0;}
.head_sns_bx li.facebook a:hover{background-position:-130px 0;}
.head_sns_bx li.youtube a:hover{background-position:-164px 0;}

/* 링크박스
=========================================================*/
.head_linkbx{float: right;padding-right:28px;height: 62px;}
.head_linkbx{padding-right:0;}/*임시*/
.head_linkbx ul:after{content: ""; display: table; clear: both;}
.head_linkbx li{float: left;padding:23px 15px;transition: all ease-out 0.25s; -webkit-transition: all ease-out 0.25s; -moz-transition: all ease-out 0.25s; -ms-transition: all ease-out 0.25s;-o-transition: all ease-out 0.25s;transform: scale(1);-webkit-transform: scale(1);-moz-transform: scale(1);-ms-transform: scale(1);}
.head_linkbx li a{font-size: 14px; color:#fff; line-height: 1.2em;font-family: 'Open Sans', sans-serif;display: block;}
.head_linkbx li:last-child{padding-right:0;}/*임시*/

/*호버*/
.head_linkbx li a:hover{color:#dd5828;}
.head_linkbx li.active a{color:#dd5828;}

/* 서치버튼
=========================================================*/
.top_headbx .search_btn{width:19px; height: 19px; position: absolute; top:50%; right:40px; margin-top:-9px;transition: all ease-out 0.25s; -webkit-transition: all ease-out 0.25s; -moz-transition: all ease-out 0.25s; -ms-transition: all ease-out 0.25s;-o-transition: all ease-out 0.25s;}
.top_headbx .search_btn{display: none;}/*임시*/
.top_headbx .search_btn a{width:19px; height: 19px;background: url(../images/icons/icon_item.png) no-repeat;background-size: 500px 500px; background-position: 0 -40px;display: block;text-indent: -99999px;}

/*호버*/
.top_headbx .search_btn a:hover{background-position: -22px -40px;}

/*=========================================================
  반응형
=========================================================*/ 
@media screen and (max-width: 1300px) and (min-width: 1024px){
.head_linkbx li{padding:24px 15px;}
.head_linkbx li a{font-size: 12px;}
}

/*=========================================================
  네비게이션
=========================================================*/ 
.nav_headbx .primary-menu{margin:0 0 0 -20px;}
.nav_headbx .primary-menu:after{content: ""; display: table; clear: both;}
.nav_headbx .primary-menu > li{float: left;}
.nav_headbx .primary-menu > li > a{font-size: 19px; color:#ffffff; line-height: 1.2em;display: block;padding:26px 5px;text-align: center;position: relative;height: 74px;}

/*조건값*/
.nav_headbx .primary-menu > li.nav_greatu{width:125px;}
.nav_headbx .primary-menu > li.nav_about{width:147px;}
.nav_headbx .primary-menu > li.nav_athlete{width:127px;}
.nav_headbx .primary-menu > li.nav_match{width:221px;}
.nav_headbx .primary-menu > li.nav_match a{padding-right:81px;}
.nav_headbx .primary-menu > li.nav_media{width:181px;}
.nav_headbx .primary-menu > li.nav_media a{padding-left:81px;}
.nav_headbx .primary-menu > li.nav_youth{width:115px;}
.nav_headbx .primary-menu > li.nav_management{width:129px;}
.nav_headbx .primary-menu > li.nav_quick{width:175px;}
.nav_headbx .primary-menu > li.nav_quick > a{padding-right:38px;font-family: 'Open Sans', sans-serif;font-weight: 600;}
.nav_headbx .primary-menu > li.nav_quick > a:after{content: "";width:20px; height: 11px; background: url(../images/icons/icon_item.png) no-repeat;background-size: 500px 500px; background-position: -52px -40px;display: block; position: absolute; top:50%; right:20px;margin-top:-5px;}

/* 퀵메뉴
=========================================================*/
.nav_headbx .primary-menu > li.nav_quick > a{background: #dd5828;}
.nav_headbx .primary-menu > li.nav_quick:hover > a{color: #fff;}

/*호버*/
.nav_headbx .primary-menu > li[class*="current"] > a{color:#dd5828;}
.nav_headbx .primary-menu > li:hover > a{color:#dd5828;}
.nav_headbx .primary-menu > li.nav_quick[class*="current"] > a{color:#fff !important;}


/* 서브 네비게이션
=========================================================*/ 
.nav_headbx .primary-menu > li .sub-menu{height: 0; overflow: hidden;transition: all ease-out 0.15s; -webkit-transition: all ease-out 0.15s; -moz-transition: all ease-out 0.15s; -ms-transition: all ease-out 0.15s;-o-transition: all ease-out 0.15s;}
.nav_headbx .primary-menu > li .sub-menu > li > a{font-size:13px; color:#fff;line-height: 1.2em;display: block; text-align: center;padding:7px 5px;}
.nav_headbx .primary-menu > li .sub-menu > li:first-child{padding-top:21px;}
.nav_headbx .primary-menu > li .sub-menu > li:last-child{padding-bottom:22px;}
.nav_headbx .primary-menu > li .sub-menu .sub-menu{display: none !important;}

/*조건값*/
.nav_headbx .primary-menu > li.nav_quick .sub-menu{background: rgba(221,88,40,0.9);}
.nav_headbx .primary-menu > li.nav_match .sub-menu > li > a{padding-right:81px;}
.nav_headbx .primary-menu > li.nav_media .sub-menu > li > a{padding-left:81px;}

/*호버*/
.nav_headbx .primary-menu > li .sub-menu > li > a:hover{color:#dd5828;}
.nav_headbx .primary-menu > li .sub-menu > li[class*="current"] > a{color:#dd5828;}
.nav_headbx .primary-menu > li.nav_quick .sub-menu > li > a:hover{color:#fff;background: rgba(221,88,40,0.9);}
.nav_headbx .primary-menu > li.nav_quick .sub-menu > li[class*="current"] > a{color:#fff;background: rgba(221,88,40,0.9);}

/*=========================================================
  반응형
=========================================================*/ 
@media screen and (max-width: 1300px) and (min-width: 1024px){
.nav_headbx .primary-menu > li.nav_greatu{width:91px;}
.nav_headbx .primary-menu > li.nav_about{width:113px;}
.nav_headbx .primary-menu > li.nav_athlete{width:93px;}
.nav_headbx .primary-menu > li.nav_match{width:187px;}
.nav_headbx .primary-menu > li.nav_media{width:147px;}
.nav_headbx .primary-menu > li.nav_youth{width:81px;}
.nav_headbx .primary-menu > li.nav_management{width:95px;}
.nav_headbx .primary-menu > li.nav_quick{width:141px;}
.nav_headbx .primary-menu > li > a{font-size: 16px;}
.nav_headbx .primary-menu > li .sub-menu > li > a{font-size: 12px;}
.nav_headbx .primary-menu > li.nav_quick > a:after{right:12px;}
.nav_headbx .primary-menu > li.nav_quick > a{padding-right:32px;}
}

/* 헤더 액션 조건값
=========================================================*/
.site-header.nav_on .hover_bg{opacity: 1; visibility: visible;}
.site-header.nav_on .nav_headbx .primary-menu > li .sub-menu{height: 248px;}
.site-header.nav_on .nav_headbx:after{opacity: 1;visibility: visible;}

/*=========================================================
  헤더 서치
=========================================================*/ 
.head_searchbx{width:100%; height: 62px;position: absolute; top:0; left:0;z-index: 20;transition: all ease-out 0.15s; -webkit-transition: all ease-out 0.15s; -moz-transition: all ease-out 0.15s; -ms-transition: all ease-out 0.15s;-o-transition: all ease-out 0.15s;opacity: 0; visibility: hidden;}
.head_searchbx:after{content: ""; display: table; clear: both;}
.head_searchbx label{display: none;}
.head_searchbx .search_anibx{width:50%; float: right;position: relative;padding:0 40px 0 121px;}
.head_searchbx .submitbx{width:19px; height: 19px;background: url(../images/icons/icon_item.png) no-repeat;background-size: 500px 500px; background-position: 0 -40px;display: block;text-indent: -99999px;position: absolute; top:50%; margin-top:-9px; left:88px;cursor: pointer;transition-delay: 200ms;-webkit-transition-delay: 200ms;-moz-transition-delay: 200ms;transition-duration: 0.5s;-webkit-transition-duration: 0.5s;-moz-transition-duration: 0.5s;}
.head_searchbx .textbx{font-size: 14px; color:#fff; line-height: 1.2em;padding:23px 0 23px 0;width:100%;height: 62px;opacity: 0;visibility: hidden; transition-delay: 280ms;-webkit-transition-delay: 280ms;-moz-transition-delay: 280ms;transition-duration: 0.5s;-webkit-transition-duration: 0.5s;-moz-transition-duration: 0.5s;}
.head_searchbx .close_btn{width:19px; height: 19px;background: url(../images/icons/icon_item.png) no-repeat;background-size: 500px 500px; background-position: -162px -40px;display: block;text-indent: -99999px;position: absolute; top:50%; margin-top:-9px; right:0px;opacity: 0;visibility: hidden; transition-delay: 280ms;-webkit-transition-delay: 280ms;-moz-transition-delay: 280ms;transition-duration: 0.5s;-webkit-transition-duration: 0.5s;-moz-transition-duration: 0.5s;}

/*placeholder
=========================================================*/
.head_searchbx .textbx:-ms-input-placeholder {font-size: 14px; color:rgba(255,255,255,0.7);line-height: 1.2em;}
.head_searchbx .textbx::-webkit-input-placeholder {font-size: 14px; color:rgba(255,255,255,0.7);line-height: 1.2em;}
.head_searchbx .textbx:-moz-placeholder {font-size: 14px; color:rgba(255,255,255,0.7);line-height: 1.2em;} 
.head_searchbx .textbx::-moz-placeholder {font-size: 14px; color:rgba(255,255,255,0.7);line-height: 1.2em;}

/* 헤더서치박스 액션 조건값
=========================================================*/
.site-header.search_on .head_searchbx{opacity: 1;visibility: visible;}
.site-header.search_on .head_searchbx .textbx{opacity: 1; visibility: visible;}
.site-header.search_on .head_searchbx .close_btn{opacity: 1; visibility: visible;}
.site-header.search_on .head_searchbx .submitbx{left:81px;}
.site-header.search_on .head_linkbx li{transform: scale(0);-webkit-transform: scale(0);-moz-transform: scale(0);-ms-transform: scale(0);}
.site-header.search_on .top_headbx .search_btn{transform: scale(0);-webkit-transform: scale(0);-moz-transform: scale(0);-ms-transform: scale(0);}

